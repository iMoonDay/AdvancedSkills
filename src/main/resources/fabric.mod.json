{
  "schemaVersion": 1,
  "id": "advanced_skills",
  "version": "${version}",
  "name": "Advanced Skills",
  "description": "A large number of skills give you a more enjoyable combat experience",
  "authors": [
    "iMoonDay"
  ],
  "contact": {
    "homepage": "https://github.com/iMoonDay/AdvancedSkills",
    "sources": "https://github.com/iMoonDay/AdvancedSkills",
    "issues": "https://github.com/iMoonDay/AdvancedSkills/issues"
  },
  "license": "MIT",
  "icon": "assets/advanced_skills/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "value": "com.imoonday.AdvancedSkillsClient",
        "adapter": "kotlin"
      }
    ],
    "main": [
      {
        "value": "com.imoonday.AdvancedSkills",
        "adapter": "kotlin"
      }
    ],
    "cardinal-components": [
      {
        "value": "com.imoonday.component.Components",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "advanced_skills.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "minecraft": ">=1.20",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": "*",
    "malilib": "*"
  },
  "custom": {
    "cardinal-components": [
      "advanced_skills:data",
      "advanced_skills:property"
    ]
  },
  "accessWidener": "advanced_skills.accesswidener"
}