{
  "schemaVersion": 1,
  "id": "advanced_skills",
  "version": "${version}",
  "name": "Advanced Skills",
  "description": "A large number of skills give you a more enjoyable combat experience",
  "authors": [
    "iMoonDay"
  ],
  "contact": {
    "homepage": "https://github.com/iMoonDay/AdvancedSkills",
    "sources": "https://github.com/iMoonDay/AdvancedSkills"
  },
  "license": "MIT",
  "icon": "assets/advanced_skills/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "value": "com.imoonday.AdvancedSkillsClient",
        "adapter": "kotlin"
      }
    ],
    "main": [
      {
        "value": "com.imoonday.AdvancedSkills",
        "adapter": "kotlin"
      }
    ],
    "cardinal-components": [
      {
        "value": "com.imoonday.init.ModComponents",
        "adapter": "kotlin"
      }
    ]
  },
  "mixins": [
    "advanced_skills.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.24",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "fabric-api": "*",
    "fabric-language-kotlin": ">=1.9.22"
  },
  "custom": {
    "cardinal-components": [
      "advanced_skills:exp",
      "advanced_skills:skills",
      "advanced_skills:equipped_skills",
      "advanced_skills:using_skills",
      "advanced_skills:damaged_time",
      "advanced_skills:status"
    ]
  },
  "accessWidener": "advanced_skills.accesswidener"
}